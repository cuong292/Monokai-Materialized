<idea-plugin
  url="https://github.com/samueljarosinski/Monokai-Materialized">

  <id>color.scheme.Monokai Materialized</id>
  <name>Monokai Materialized Theme</name>
  <version>1.2</version>

  <vendor
    email="samueljarosinski@gmail.com"
    url="https://github.com/samueljarosinski">
    Samuel Jarosi≈Ñski
  </vendor>

  <description>
    <![CDATA[
      Color scheme inspired by Monokai, filled with Material Design colors.
    ]]>
  </description>

  <change-notes>
    <![CDATA[
      <ul>
        <li>Restore default gutter color</li>
        <li>Set followed hyperlink color to same as normal hyperlink</li>
      </ul>
    ]]>
  </change-notes>

  <idea-version since-build="142.0" />

  <depends>com.intellij.modules.lang</depends>

  <extensions
    defaultExtensionNs="com.intellij">

    <bundledColorScheme
      path="/colors/Monokai Materialized" />

    <themeProvider
      id="50e7efe7-41f5-41ad-b654-1bb5efaf6585"
      path="/themes/Monokai Materialized.theme.json" />

  </extensions>

</idea-plugin>
